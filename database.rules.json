{
  "rules": {
    ".read": "auth !== null",
    ".write": "auth !== null",
    "displayNames": {
      ".read": true,
      ".write": false
    },
    "serviceAccounts": {
      "$serviceAccountId": {
        ".read": "auth !== null",
        ".write": "auth !== null"
      }
    },
    "users": {
      "$uid": {
        ".write": "auth !== null && $uid === auth.uid"
      }
    }
  }
}
